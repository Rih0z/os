{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "serena"
      ]
    },
    "qemu-vm": {
      "command": "uv",
      "args": [
        "--directory",
        "/home/kokiriho/lern/mcp-qemu-vm",
        "run",
        "server.py"
      ],
      "env": {
        "VM_HOST": "192.168.122.79",
        "VM_USER": "vmrobot",
        "VM_PORT": "22",
        "VM_DISPLAY": ":0"
      }
    }
  }
}
